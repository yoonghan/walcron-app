{
  "version": 2,
  "regions": ["sin1"],
  "rewrites": [
    {
      "source": "/service-worker.js",
      "destination": "/_next/static/service-worker.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key": "Link",
          "value": "</css/main.css>; rel=preload; as=style"
        }
      ]
    },
    {
      "source": "/css/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/pwa/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
  },
  "build": {
    "env": {
      "USER_ID": "1",
      "BACKEND_SERVER": "https://service.walcron.com"
    }
  }
}
